{
  "kind": "collectionType",
  "collectionName": "users-permissions_user",
  "info": {
    "name": "user",
    "description": ""
  },
  "options": {
    "privateAttributes": [
      "username"
    ],
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "model": "role",
      "via": "users",
      "plugin": "users-permissions",
      "configurable": false
    },
    "phone": {
      "type": "string",
      "regex": "^\\+((?:9[679]|8[035789]|6[789]|5[90]|42|3[578]|2[1-689])|9[0-58]|8[1246]|6[0-6]|5[1-8]|4[013-9]|3[0-469]|2[70]|7|1)(?:\\W*\\d){0,13}\\d$",
      "required": true,
      "unique": true,
      "private": true
    },
    "phoneToken": {
      "type": "integer",
      "private": true
    },
    "emailToken": {
      "type": "integer",
      "private": true
    },
    "onboarded": {
      "type": "boolean",
      "default": false
    },
    "onboardedAt": {
      "type": "datetime",
      "private": true
    },
    "profile": {
      "via": "user",
      "model": "profile"
    },
    "lastLogin": {
      "type": "datetime"
    },
    "discussions": {
      "via": "user",
      "collection": "discussion"
    },
    "discussion_replies": {
      "via": "user",
      "collection": "discussion-reply"
    }
  }
}
